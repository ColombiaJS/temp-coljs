{
	// Enforcing Options
	"bitwise": true,
	"camelcase": true,
	"curly": false,
	"immed": true,
	"indent": 4,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"undef": true,
	"unused": true,
	"strict": true,
	// "trailing": true,
	"maxparams": 4,
	"maxlen": 80,
	// using lastsemic instead of asi
	// "asi": true,
	"evil": true,

	// Relaxing Options
	"expr": true,
	"lastsemic": true,
	"multistr": true,

	// Environment Options
	"node": true,
	"jquery": true,
	"browser": true,
	// "esnext": true,

	"globals": {
		"jQuery": true,
		"$": true,

		"module": true,
		"require": true,
		"requirejs": true,
		"define": true,

		"describe": true
	}
}